<%=simple_form_for [@taxpayer, @land], url: :taxpayer_lands do |f| %>
  <%=f.error_notification %>
  <%=f.input :arp_number %>
  <%=f.input :land_area %>
  <%=f.simple_fields_for :address do |a| %>
    <%=a.input :street %>
    <%=a.input :municipality, collection: Address::MUNICIPALITIES %>
  <% end %>
  <%=f.simple_fields_for :administrator do |aa| %>
    <%=aa.input :first_name %>
  <% end %>
  <%=f.submit 'Save' %>
<% end %>
